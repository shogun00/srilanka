{
  "version": 2,
  "builds": [{
    "src": "package.json",
    "use": "@now/next"
  }],
  "routes": [{
      "src": "/projects/(?<project_id>[^/]+)/issues/(?<id>[^/]+)$",
      "dest": "/projects/issues/Issue?project_id=$project_id&id=$id"
    },
    {
      "src": "/projects/(?<id>[^/]+)/issues/new",
      "dest": "/projects/issues/NewIssue?id=$id"
    },
    {
      "src": "/projects/(?<id>[^/]+)$",
      "dest": "/projects/project?id=$id"
    }
  ]
}
